    "eslint-plugin-vue": "^9.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.3",
    "prettier": "^2.6.2",
    "sass": "^1.52.1",
    "typescript": "^4.7.2",
    "unplugin-auto-import": "^0.8.6",
    "unplugin-vue-components": "^0.19.6",
