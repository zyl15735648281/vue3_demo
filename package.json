    "element-plus": "^2.2.2",
    "pinia": "^2.0.14",
    "qs": "^6.10.3",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue": "^3.2.25",
    "vue-eslint-parser": "^9.0.2",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@commitlint/cli": "^17.0.1",
